    <config_file>
      <file_path>/usr/local/bin/urlexec</file_path>
      <file_contents>
<![CDATA[#!/usr/bin/perl -w
#$Revision: 57099 $$Date: 2013-01-14 14:48:55 -0500 (Mon, 14 Jan 2013) $$Author: ygiang $
######################################################################
#
# Copyright 2005 by Jeff Dutton <jdutton@cpan.org>.  This program is
# free software; you can redistribute it and/or modify it under
# the terms of either the GNU Lesser General Public License or the Perl
# Artistic License.
#
# This program is distributed in the hope that it will be useful,
# but WITHOUT ANY WARRANTY; without even the implied warranty of
# MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the
# GNU General Public License for more details.
#                                                                           
######################################################################

use IO::Handle;
use Getopt::Long;
use Cwd qw();
use strict;
use vars qw($Opt_Cd @CmdParts);

if (! GetOptions ( "cd=s"	=> \$Opt_Cd,
		   "<>"		=> \&parameter,
		   )) {
    usage();
}

sub parameter {
    my $param = shift;
    if ($CmdParts[0] || $param !~ /^-/) {
	push @CmdParts, $param;
    } else {
	print "%Error: urlexec: Unknown option: $param\n";
	usage();
    }
}

sub usage {
    #print '$Id: autoinst.xml 57099 2013-01-14 19:48:55Z ygiang $\n';
    pod2usage(-verbose=>2, -exitval => 2);
    exit (1);
}

push (@CmdParts, @ARGV); # Any remaining arguments (i.e. after a -- on the command line)
@CmdParts or die "%Error: urlexec: No command argument was specified!\n";

if ($Opt_Cd) {
    Cwd::chdir($Opt_Cd)
	or die "%Error: urlexec: Could not chdir to \"$Opt_Cd\"!\n";
}

# This program treats all its arguments as a URL/percent encoded
#   command.  It therefore decodes all arguments and exec's them.
my $cmd = join(' ', @CmdParts);
$cmd =~ tr/+/ /; # Plus encodes a space
$cmd =~ s/%([a-fA-F0-9]{2,2})/chr(hex($1))/eg; # Decode the % encoding
autoflush STDOUT;
autoflush STDERR;
exec('/bin/sh', '-c', $cmd);
exit 1;  # This will never happen

#######################################################################
__END__

=pod

=head1 NAME

urlexec - Decode and execute the given URL-encoded command

=head1 SYNOPSIS

  urlexec [--cd I<cd>] echo %24HOME

=head1 DESCRIPTION

Run the specified command (optionally from the specified directory).
Each URL-encoded character (%xx) is translated prior to executing
the command with exec().

=head1 ARGUMENTS

=over 4

=item --cd I<dir>

Directory to cd to before the exec of the given command.

=back

=head1 AUTHORS

Jeff Dutton <jdutton@cpan.org>

=head1 SEE ALSO

L<AutoTest>

=cut
]]>
      </file_contents>
    </config_file>
    <config_file>
      <file_script>
      <interpreter>shell</interpreter>
      <source>
<![CDATA[
!#/bin/bash
chmod 0755 /usr/local/bin/urlexec
]]>
      </source>
    </file_script>
    </config_file>
    <config_file>
      <file_script>
      <interpreter>shell</interpreter>
      <source>
<![CDATA[
!#/bin/bash
chkconfig open-iscsi on
chkconfig open-iscsi 012356
/etc/rc.d/open-iscsi start

chkconfig --level 012356 sshd on
service sshd start
]]>
      </source>
    </file_script>
    </config_file>
  </files>
  <scripts>
    <chroot-scripts config:type="list">
      <script>
        <chrooted config:type="boolean">true</chrooted>
        <source>
<![CDATA[
!#/bin/bash
grep -v 'net.ipv4.conf.default.rp_filter\|net.ipv4.conf.all.rp_filter\|enable route verification on all interfaces' /etc/sysctl.conf > /etc/sysctl.conf.tmp
echo '' >> /etc/sysctl.conf.tmp
echo '# relaxed route verification on all interfaces' >> /etc/sysctl.conf.tmp
echo 'net.ipv4.conf.default.rp_filter = 2' >> /etc/sysctl.conf.tmp
echo 'net.ipv4.conf.all.rp_filter = 2' >> /etc/sysctl.conf.tmp
mv /etc/sysctl.conf.tmp /etc/sysctl.conf
]]>
        </source>
      </script>
    </chroot-scripts>
  </scripts>
</profile>
